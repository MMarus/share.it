extends layout

block content
    div.container-fluid
        div.col-md-2
            div.panel
                button.btn.btn-default
                    i.material-icons school
    h1= 'Welcome to ' + title
    p Who are you?
    - each item in rows
        button.btn.btn-default(onclick="setUser({id:" + item.id + ", name: '"+ item.name +"'})") #{item.name}
